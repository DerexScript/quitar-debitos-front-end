{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"plugin:react/recommended",
		"standard-with-typescript",
		"plugin:@typescript-eslint/recommended"
	],
	"overrides": [
		{
			"files": ["*.ts", "*.tsx"]
		}
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"project": ["./"]
	},
	"plugins": ["react", "@typescript-eslint", "prettier"],
	"rules": {}
}
